@model string

<div class="container page-container">
    <div class="d-flex flex-column justify-content-center align-items-center text-center px-3">
        <h1 class="display-4 app-error"><i class="bx bx-error mt-3"></i>Error</h1>
			<p class="error-description">
				@if ( Model != null )
				{
					<span>@Model</span>
				}
				else
				{
					<span>Ein unbekannter Fehler ist aufgetreten.</span>
				}
			</p>
    </div>
</div>